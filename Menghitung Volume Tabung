/*Program Untuk Menghitung Volume Tabung*/
#include<iostream>
using namespace std;
int main(){
    int r,t;
    float phi=3.14,volume;
    cout<<"Masukkan jari jari :";
    cin>>r;
    cout<<"Masukkan Tinggi Tabung :";
    cin>>t;
    volume = phi*r*r*t;
    cout<<"Volume Tabung adalah :"<<volume;
    return 0;
}
